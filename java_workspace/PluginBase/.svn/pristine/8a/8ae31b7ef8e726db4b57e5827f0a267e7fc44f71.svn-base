package com.crestech.opkey.plugin.communication.transport;

import java.io.IOException;

public class ChannelClosedException extends IOException {
	
	private static final long serialVersionUID = -4790378968487851976L;

	public ChannelClosedException() {
		super("The underlying communication channel has closed or broken.");
	}
	
	public ChannelClosedException(Throwable e) {
		super("The underlying communication channel has closed or broken.", e);
	}
}
